        },
        {
          "name": "1s",
          "id": "e01bce9d-a360-4860-a1e1-47da772a1648",
          "type": "wait",
          "time": [
            "return 1"
          ]
        }
      ],
      "decorators": [
        {
          "id": "4daa9047-4b52-4bb6-8640-b9a1cf8a4394",
          "type": "scriptCondition",
          "name": "target not visible, but clear spot closer to target",
          "script": [
            "return loc.tacticalAnalysis.targetToMove ~= nil"
          ]
        }
      ]
