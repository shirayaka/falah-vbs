                      "subtrees": [
                        {
                          "name": "suborderData",
                          "id": "75ae5b84-dca6-4226-8549-e01b6105fa97",
                          "type": "scriptAction",
                          "script": [
                            "local targets = bb.targets",
                            "local closestTargetID = 1",
                            "local closestTargetDistance = math.huge",
                            "",
                            "for i=1, #targets do",
                            "    local distance = self:GetPosition():Distance(targets[i]:GetPosition())",
                            "    if ",
